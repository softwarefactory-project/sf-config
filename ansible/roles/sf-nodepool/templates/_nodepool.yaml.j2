script-dir: /etc/nodepool/scripts
elements-dir: /etc/nodepool/elements:/usr/share/sf-elements:/usr/share/nodepool/elements
images-dir: /var/lib/nodepool/dib

zmq-publishers:
{% for publisher in jobs_zmq_publishers %}
  - {{ publisher }}
{% endfor %}

gearman-servers:
  - host: {{ zuul_server_host }}

cron:
  image-update: '14 2 * * *'
  cleanup: '*/30 * * * *'
  check: '*/15 * * * *'

targets:
  - name: default

labels: []
providers: []
diskimages: []
