# This file is managed by ansible, do not edit directly
---
- tenant:
    name: 'local'
    source:
      {{ config_connection_name }}:
        config-projects:
          - {{ config_project_name }}
        untrusted-projects:
          - {{ jobs_project_name }}

- tenant:
    name: 'local'
    source:
{% if zuul_upstream_zuul_jobs %}
      {{ openstack_connection_name }}:
        untrusted-projects:
          - openstack-infra/zuul-jobs:
              include:
                - job
              shadow: {{ jobs_project_name }}
{% else %}
      {{ zuul_jobs_connection_name }}:
        untrusted-projects:
          - zuul-jobs:
              shadow: {{ jobs_project_name }}
{% endif %}
