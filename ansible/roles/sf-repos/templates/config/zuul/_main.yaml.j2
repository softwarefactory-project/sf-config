# This file is managed by ansible, do not edit directly
---
{% if not tenant_deployment %}
- tenant:
    name: '{{ tenant_name }}'
    source:
      {{ config_connection_name }}:
        config-projects:
          - {{ config_project_name }}
        untrusted-projects:
          - {{ sf_jobs_project_name }}

- tenant:
    name: '{{ tenant_name }}'
    source:
      {{ zuul_jobs_connection_name }}:
        untrusted-projects:
          - {{ zuul_jobs_project_name }}:
              include:
                - job
              shadow: {{ sf_jobs_project_name }}
{% endif %}
