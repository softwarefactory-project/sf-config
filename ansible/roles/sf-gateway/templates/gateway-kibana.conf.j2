<IfModule mod_proxy.c>
    <Location /app/kibana>
        ProxyPass http://kibana.{{ fqdn }}:5601/app/kibana
        ProxyPassReverse http://kibana.{{ fqdn }}:5601/app/kibana
    </Location>
    <Location /elasticsearch>
        ProxyPass http://elasticsearch.{{ fqdn }}:9200
        ProxyPassReverse http://elasticsearch.{{ fqdn }}:9200
    </Location>
</IfModule>
