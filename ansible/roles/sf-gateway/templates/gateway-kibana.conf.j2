<IfModule mod_proxy.c>
    ProxyPass /app/kibana http://kibana.{{ fqdn }}:5601
    ProxyPassReverse /app/kibana http://kibana.{{ fqdn }}:5601

    ProxyPass /elasticsearch http://elasticsearch.{{ fqdn }}:9200
    ProxyPassReverse /elasticsearch http://elasticsearch.{{ fqdn }}:9200

    Alias /bundles /usr/share/kibana/optimize/bundles

    <Directory /usr/share/kibana>
        Require all granted
    </Directory>
</IfModule>
